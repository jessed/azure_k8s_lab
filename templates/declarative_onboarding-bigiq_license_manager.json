{
  "schemaVersion": "1.7.0",
  "class": "Device",
  "async": true,
  "label": "Onboard BIG-IP",
  "Common": {
    "class": "Tenant",
    "hostname": "${local_host}",
    "myLicenses": {
      "class": "License",
      "bigIpUsername":  "${bigIpUser}",
      "bigIpPassword":  "${bigIpPass}",
      "bigIqHost":      "${bigIqHost}",
      "bigIqUsername":  "${bigIqUsername}",
      "bigIqPassword":  "${bigIqPassword}",
      "licenseType":    "${bigIqLicenseType}",
      "licensePool":    "${bigIqLicensePool}",
      "unitOfMeasure":  "${bigIqUnitOfMeasure}",
      "reachable":      false,
      "hypervisor":     "${bigIqHypervisor}",
      "overwrite":      true
    },
    "dbVars": {
      "class": "DbVariables",
      "ui.advisory.enabled": true,
      "ui.advisory.color": "green",
      "ui.advisory.text": "/Common/hostname",
      "config.allow.rfc3927": "enable"
    },
    "analytics": {
      "offboxEnabled": true,
      "offboxProtocol": "tcp",
      "offboxAddresses": "127.0.0.1",
      "offboxTcpPort": 6514
    },
    "myDns": {
      "class": "DNS",
      "nameServers": [ "168.63.129.16" ],
      "search": [ "westus2.cloudapp.azure.com" ]
    },
    "myNtp": {
      "class": "NTP",
      "servers": [ "tick.ucla.edu" ],
      "timezone": "America/Los_Angeles"
    },
    "myProvisioning": {
      "class": "Provision",
      "ltm": "nominal",
      "afm": "nominal",
      "avr": "nominal"
    },
    "external": {
      "class": "VLAN",
      "tag": 4094,
      "mtu": 1500,
      "interfaces": [
        {
          "name": "1.1", "tagged": false
        }
      ]
    },
    "ext-selfip": {
      "class": "SelfIp",
      "address": "${local_selfip}/24",
      "vlan": "external",
      "allowService": "default",
      "trafficGroup": "traffic-group-local-only"
    },
    "default": {
      "class": "Route",
      "gw": "${data_gateway}",
      "network": "default",
      "mtu": 1500
    }
  }
}
